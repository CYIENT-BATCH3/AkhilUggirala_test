// WAP that adds, subtracts and divides two numbers.

#include<stdio.h>
int main()
{
int a,b; 
int sum,diff,mul;
float div;
    scanf("%d %d",&a,&b);
    sum=a+b;
    printf("Addition: a+b=%d\n",sum);                
    diff=a-b;
    printf("Subtraction: a-b=%d \n",diff);
    div=a/b;
    printf("Division: a/b=%f \n",div);
    mul=a*b;
    printf("Multipication: a*b=%d \n",mul);
}
